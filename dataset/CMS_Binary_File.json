{
	"name": "CMS_Binary_File",
	"properties": {
		"linkedServiceName": {
			"referenceName": "CMSDatalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string"
			},
			"FileType": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileType",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('Staging/', dataset().FileType)",
					"type": "Expression"
				},
				"fileSystem": "census"
			}
		}
	}
}